    "test": "karma start karma.config.js",
    "watch": "tsc --watch & webpack --watch"
  },
  "author": {
    "name": "Abraham Williams",
    "email": "abraham@abrah.am",
    "url": "https://abrah.am"
  },
  "bugs": {
    "url": "https://github.com/abraham/nutmeg-cli/issues"
  },
  "homepage": "https://nutmeg.tools",
