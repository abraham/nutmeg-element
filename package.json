    "lit-html": "0.8.0",
    "reflect-metadata": "0.1.12"
  },
  "devDependencies": {
    "@types/chai": "4.1.2",
    "@types/mocha": "2.2.48",
    "@webcomponents/webcomponentsjs": "1.1.0",
    "chai": "4.1.2",
    "karma": "2.0.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-firefox-launcher": "1.1.0",
    "karma-mocha": "1.3.0",
    "karma-sinon": "1.0.5",
    "karma-typescript": "3.0.11",
    "karma-typescript-es6-transform": "1.0.3",
    "mocha": "5.0.0",
    "sinon": "4.2.2",
    "typescript": "2.7.1",
    "uglifyjs-webpack-plugin": "1.1.8",
    "webpack": "3.10.0"
  }
