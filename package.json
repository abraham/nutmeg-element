{
  "name": "@nutmeg/seed",
  "version": "0.8.0",
  "description": "Nutmeg Web Component Seed",
  "main": "dist/seed.js",
  "module": "dist/seed.js",
  "unpkg": "dist/seed.min.js",
  "types": "dist/seed.d.ts",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abraham/nutmeg-seed.git"
  },
  "keywords": [
    "web-components",
    "webcomponents",
    "shadow-dom",
    "shadowdom",
    "lit-html",
    "nutmeg",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.production.json && webpack",
    "clean": "rm -rf dist/*",
